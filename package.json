{
  "name": "react-weather-dashboard",
  "version": "1.0.0",
  "description": "React Weather Dashboard",
  "keywords": [
    "dashboard",
    "weather"
  ],
  "homepage": "https://vanillaslice.github.io/ReactWeatherDashboard/",
  "bugs": {
    "url": "https://github.com/vanillaSlice/ReactWeatherDashboard/issues",
    "email": "mikelowedev@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Mike Lowe",
    "email": "mikelowedev@gmail.com",
    "url": "https://www.mikelowe.xyz/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vanillaSlice/ReactWeatherDashboard.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "run-s test:lint test:unit",
    "test:lint": "run-s test:lint:js test:lint:css",
    "test:lint:js": "eslint --ext=js --ext=jsx src/",
    "test:lint:css": "stylelint src/**/*.css",
    "test:unit": "react-scripts test --env=jsdom --coverage --colors",
    "coveralls": "cat coverage/lcov.info | node node_modules/.bin/coveralls",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^3.3.7",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "font-awesome": "^4.7.0",
    "google-map-react": "^1.1.1",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.6.3",
    "react-fontawesome": "^1.6.1",
    "react-redux": "^6.0.0",
    "react-scripts": "2.1.1",
    "react-sparklines": "^1.7.0",
    "redux": "^4.0.1",
    "redux-promise": "^0.6.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^16.6.3",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "private": true,
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.jsx",
      "src/**/*.js",
      "!src/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    }
  }
}
